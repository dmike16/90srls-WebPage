{
  "compileOnSave":false,
  "compilerOptions": {
    "outDir": "./build/ux",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noImplicitAny": true,
    "target": "esnext",
    "module": "commonjs",
    "typeRoots": [
      "./node_modules/@types",
      "./src/types"
    ],
    "lib": [
      "ESNEXT",
      "DOM"
    ]
  },
    "include": [
    "./**/*.ts"
  ]
}
