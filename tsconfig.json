{
  "compileOnSave":false,
  "compilerOptions": {
    "outDir": "./build/ux",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noImplicitAny": true,
    "target": "esnext",
    "module": "commonjs",
    "lib": [
      "ESNEXT",
      "DOM"
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    }
  },
    "include": [
    "./**/*.ts"
  ]
}
