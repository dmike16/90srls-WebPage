{
  "compileOnSave":false,
  "compilerOptions":{
    "module": "ES6",
    "target": "ES5",
    "removeComments":false,
    "emitDecoratorMetadata":true,
    "experimentalDecorators":true,
    "sourceMap": true,
    "noImplicitAny":true,
    "alwaysStrict": true,
    "declaration": false,
    "lib":[
      "DOM","ES6","DOM.Iterable","ScriptHost"
    ],
    "types": ["node","material-design-lite"]
  },
  "include":[
    "src/**/*"
  ]
}
